{
  "name": "clash-auto-switch",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.1",
  "description": "pkg-description",
  "author": "Lu-Jiejie <lu-jiejie@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/Lu-Jiejie/clash-auto-switch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lu-Jiejie/clash-auto-switch.git"
  },
  "bugs": "https://github.com/Lu-Jiejie/clash-auto-switch/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "cron": "^3.1.7",
    "dotenv": "^16.4.5",
    "ky": "^1.2.4",
    "picocolors": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
